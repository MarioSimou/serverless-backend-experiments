{
  "name": "mongodb-realm-todo-list",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start:dev": "turbo run start:dev --parallel",
    "deploy": "turbo run deploy",
    "lint": "turbo run lint",
    "prettier:check": "prettier \"**/*.{ts,tsx,md}\"",
    "prettier:format": "pnpm prettier:check --write",
    "clean": "rm -rf node_modules 2> /dev/null && turbo run clean --parallel"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.3.0"
}
